{
    "verbose": true,
    "restartable": "rs",
    "ignore": [
        ".git",
        "*/**.res",
        "node_modules/**/node_modules"
      ],
    "watch": ["src/"],
    "delay": "3",
    "execMap": {
        "js": "node"
    },
    "env": {
        "NODE_ENV": "development"
      },
      "ext": "js,json"
}